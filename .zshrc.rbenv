export RBENV_ROOT=/usr/local/rbenv
export PATH=$RBENV_ROOT/bin:/usr/local/rbenv/plugins/ruby-build/bin:$PATH

case $SHELL in
*/zsh)
  source $RBENV_ROOT/completions/rbenv.zsh
  ;;
*/bash)
  source $RBENV_ROOT/completions/rbenv.bash
  ;;
*)
  source $RBENV_ROOT/completions/rbenv.bash
  ;;
esac

eval "$(rbenv init -)"

